<h1>Nsturesults#index</h1>
<p>Find me in app/views/nsturesults/index.html.erb</p>


<%= form_tag nsturesults_path, :method => 'get' do %>
<%= text_field_tag :search, params[:search], class: "span12 search_input", placeholder: "Roll no." %>
<%= submit_tag "Check", :class=>"btn btn-primary btn-success" ,:title => nil %>
<% end %>

 <% @examineesdetails.each do |details| %>
<tr>
<td><%= details.name %></td>
  <td><%= details.examinee.unit %></td>
  <td><%= details.examinee.roll_number %></td>
<% end %>
</tr>